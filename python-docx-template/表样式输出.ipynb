    "from docx import Document\n",
    "from docx.enum.style import WD_STYLE_TYPE\n",
    "\n",
    "# https://www.cnblogs.com/xtfge/p/9949054.html\n",
    "# 输出全部表样式\n",
    "d = Document()\n",
    "styles = d.styles\n",
    "for s in styles:\n",
    "    if s.type == WD_STYLE_TYPE.TABLE:\n",
    "        print(s.name)"
   ]
